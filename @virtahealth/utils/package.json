{
  "name": "@virtahealth/utils",
  "version": "10.19.2",
  "description": "Common utilities for Virta front-end projects",
  "author": "Kevin Bullock <kevin.bullock@virtahealth.com>",
  "scripts": {
    "start": "env NODE_ENV=development webpack --config ./development.config.js --watch",
    "build": "env NODE_ENV=development webpack --config ./development.config.js",
    "build-prod": "env NODE_ENV=production webpack --config ./production.config.js --optimize-minimize",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "test": "jest",
    "test-watch": "jest --watch",
    "verify-translations": "ts-node --transpile-only ./src/translations/verifyTranslations.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "src/index.ts",
  "license": "UNLICENSED",
  "dependencies": {
    "@apollo/client": "3.3.21",
    "date-fns": "^2.25.0",
    "fraction.js": "^4.1.1",
    "graphql": "15.5.1",
    "lodash": "^4.17.21",
    "normalizr": "^3.6.1",
    "react-native-web-hooks": "^3.0.1",
    "url-parse": "1.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@types/url-parse": "1.4.4",
    "babel-jest": "26.6.3",
    "babel-loader": "^8.2.1",
    "mockdate": "3.0.5",
    "react-hot-loader": "^4.13.0",
    "react-native-device-info": "^8.4.8",
    "ts-loader": "^8.0.11",
    "typescript": "4.0.5",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "optionalDependencies": {
    "react-native-device-info": "^8.4.8"
  }
}
